<!DOCTYPE html>
<html>
  <head>
    <title>test</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <style>
      body{
        margin: none;
      }
      table{
        border-collapse: collapse;
      }
      th{
        background-color: floralwhite;
        padding: 20px;
        padding-right: 40px;
        padding-left: 40px;
      }
      td{
        padding: 10px;
      }
      th, td{
        text-align: center;
      }
      .logout{
        float: right;
        font-weight: bold;  
        font-size: 20px;
        text-decoration: none;
        color: thistle;
        text-shadow: 0px 1px #ffffff, 1px 1px 0px #dad7d7;
      }
      h1{
        font-family: HY동녘M;
        font-size: 42px;
        color: grey;
        text-shadow: 0px 1px #ffffff, 3px 3px 0px #dad7d7;
        text-align: center;
      }
      .delete{
        font-size: 17px;
        text-decoration: none;
        color: black;
        text-shadow: 0px 1px #ffffff, 1px 1px 0px #dad7d7;
      }
    </style> 
  </head>
  <body>
  <a href="/logout" class="logout">로그아웃</a>
  <center>
    <table border="1">
        <tr>
          <h1>이메일 목록</h1>
          <th>이메일</th>
        </tr>
        <% for (const result of results) { %>
        <tr>
          <td><%= result.email %></td>
        </tr>
      <% } %>
    </table>
  </center>
  </body>
</html>
